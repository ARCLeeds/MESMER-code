<?xml version="1.0" encoding="utf-8" ?>
<?xml-stylesheet type='text/xsl' href='mesmer1.xsl'?>
<me:mesmer xmlns="http://www.xml-cml.org/schema" xmlns:me="http://www.chem.leeds.ac.uk/mesmer">
    <moleculeList>
        <molecule id="OH_acetylene_adduct">
            <atomarray>
                <atom id="a1" elementType="C" />
                <atom id="a2" elementType="C" />
                <atom id="a3" elementType="O" />
                <atom id="a4" elementType="H" />
                <atom id="a5" elementType="H" />
                <atom id="a6" elementType="H" />
            </atomarray>
            <bondarray>
                <bond atomRefs2="a1 a2" order="2" />
                <bond atomRefs2="a1 a3" order="1" />
                <bond atomRefs2="a3 a4" order="1" />
                <bond atomRefs2="a1 a5" order="1" />
                <bond atomRefs2="a2 a6" order="1" />
            </bondarray>
            <propertyList>
                <property dictRef="me:ZPE">
                    <scalar units="kJ/mol">0.0</scalar>
                </property>
                <property dictRef="me:rotConsts">
                    <array units="cm-1">0.32031 0.36452 2.64058</array>
                </property>
                <property dictRef="me:symmetryNumber">
                    <scalar>1</scalar>
                </property>
                <property dictRef="me:vibFreqs">
                    <array units="cm-1">464.0 474.0 567.0 770.0 848.0 1099.0 1244.0 1354.0 1667.0 3101.0 3300.0 3797.0</array>
                </property>
                <property dictRef="me:MW">
                    <scalar units="amu">43</scalar>
                </property>
                <property dictRef="me:epsilon">
                    <scalar>250.0</scalar>
                </property>
                <property dictRef="me:sigma">
                    <scalar>5.0</scalar>
                </property>
                <property dictRef="me:deltaEDown">
                    <scalar units="cm-1">250.0</scalar>
                </property>
                <property dictRef="me:spinMultiplicity">
                    <scalar>2</scalar>
                </property>
            </propertyList>
            <me:DOSCMethod>Classical rotors</me:DOSCMethod>
        </molecule>
        <molecule id="TS_1a">
            <atomarray>
                <atom id="a1" elementType="C" />
                <atom id="a2" elementType="C" />
                <atom id="a3" elementType="H" />
                <atom id="a4" elementType="H" />
                <atom id="a5" elementType="O" />
                <atom id="a6" elementType="H" />
            </atomarray>
            <bondarray>
                <bond atomRefs2="a1 a2" order="3" />
                <bond atomRefs2="a1 a3" order="1" />
                <bond atomRefs2="a2 a4" order="1" />
                <bond atomRefs2="a5 a6" order="1" />
            </bondarray>
            <propertyList>
                <property dictRef="me:ZPE">
                    <scalar units="kJ/mol">141.4</scalar>
                </property>
                <property dictRef="me:rotConsts">
                    <array units="cm-1">0.33065 0.39313 2.06178</array>
                </property>
                <property dictRef="me:symmetryNumber">
                    <scalar>1.0</scalar>
                </property>
                <property dictRef="me:vibFreqs">
                    <array units="cm-1">332.0 807.0 903.0 1010.0 1098.0 1158.0 1306.0 1407.0 1988.0 3127.0 3167.0</array>
                </property>
                <property dictRef="me:MW">
                    <scalar units="amu">43</scalar>
                </property>
                <property dictRef="me:spinMultiplicity">
                    <scalar>2</scalar>
                </property>
            </propertyList>
            <me:DOSCMethod>Classical rotors</me:DOSCMethod>
        </molecule>
        <molecule id="TS_0">
            <propertyList>
                <property dictRef="me:ZPE">
                    <scalar units="kJ/mol">136.075</scalar>
                </property>
                <property dictRef="me:MW">
                    <scalar units="amu">43</scalar>
                </property>
            </propertyList>
            <me:DOSCMethod>Classical rotors</me:DOSCMethod>
        </molecule>
        <molecule id="OH">
            <atomarray>
                <atom id="a1" elementType="O" />
                <atom id="a2" elementType="H" />
            </atomarray>
            <bondarray>
                <bond atomRefs2="a1 a2" order="1" />
            </bondarray>
            <propertyList>
                <property dictRef="me:ZPE">
                    <scalar units="kJ/mol">0.0</scalar>
                </property>
                <property dictRef="me:rotConsts">
                    <array units="cm-1">19.2438</array>
                </property>
                <property dictRef="me:symmetryNumber">
                    <scalar>1</scalar>
                </property>
                <property dictRef="me:vibFreqs">
                    <array units="cm-1">3722.0</array>
                </property>
                <property dictRef="me:MW">
                    <scalar units="amu">17</scalar>
                </property>
                <property dictRef="me:spinMultiplicity">
                    <scalar>2</scalar>
                </property>
                <property dictRef="me:eletronicExcitation">
                    <array units="cm-1">139.7</array>
                </property>
            </propertyList>
            <me:DOSCMethod>Classical rotors</me:DOSCMethod>
        </molecule>
        <molecule id="acetylene">
            <atomarray>
                <atom id="a1" elementType="C" />
                <atom id="a2" elementType="C" />
                <atom id="a3" elementType="H" />
                <atom id="a4" elementType="H" />
            </atomarray>
            <bondarray>
                <bond atomRefs2="a1 a2" order="3" />
                <bond atomRefs2="a1 a3" order="1" />
                <bond atomRefs2="a2 a4" order="1" />
            </bondarray>
            <propertyList>
                <property dictRef="me:ZPE">
                    <scalar units="kJ/mol">130.7</scalar>
                </property>
                <property dictRef="me:rotConsts">
                    <array units="cm-1">1.19205</array>
                </property>
                <property dictRef="me:symmetryNumber">
                    <scalar>2</scalar>
                </property>
                <property dictRef="me:vibFreqs">
                    <array units="cm-1">664.0 664.0 766.0 766.0 2066.0 3408.0 3509.0</array>
                </property>
                <property dictRef="me:MW">
                    <scalar units="amu">26</scalar>
                </property>
                <property dictRef="me:spinMultiplicity">
                    <scalar>1</scalar>
                </property>
            </propertyList>
            <me:DOSCMethod>Classical rotors</me:DOSCMethod>
        </molecule>
        <molecule id="N_2">
            <atom elementType="N_2" />
            <propertyList>
                <property dictRef="me:epsilon">
                    <scalar>48.0</scalar>
                </property>
                <property dictRef="me:sigma">
                    <scalar>3.90</scalar>
                </property>
                <property dictRef="me:MW">
                    <scalar units="amu">28.0</scalar>
                </property>
            </propertyList>
        </molecule>
    </moleculeList>
    <reactionList>
        <reaction id="R1">
            <reactant>
                <molecule ref="OH" me:type="reactant" />
            </reactant>
            <reactant>
                <molecule ref="acetylene" me:type="excessReactant" />
            </reactant>
            <product>
                <molecule ref="OH_acetylene_adduct" me:type="modelled" />
            </product>
            <me:transitionState>
                <molecule ref="TS_0" me:type="transitionState" />
            </me:transitionState>
            <!--Use zero point energies instead-->
            <me:MCRCMethod>Mesmer ILT</me:MCRCMethod>
            <me:preExponential>7.00e-12</me:preExponential>
            <me:excessReactantConc>1.00e17</me:excessReactantConc>
            <me:nInfinity>0.0</me:nInfinity>
        </reaction>
        <reaction id="R2">
            <reactant>
                <molecule ref="OH_acetylene_adduct" me:type="reactant" />
            </reactant>
            <me:transitionState>
                <molecule ref="TS_1a" me:type="transitionState" />
            </me:transitionState>
            <!--Use zero point energies instead-->
            <me:MCRCMethod>Simple RRKM</me:MCRCMethod>
        </reaction>
    </reactionList>
    <me:conditions>
        <me:bathGas>N_2</me:bathGas>
        <me:CPTs>
          <me:CPTpair me:units="Torr" me:CP="1000" me:T="298.0" />
        </me:CPTs>
    </me:conditions>
    <me:modelParameters>
        <!--Specify grain size directly...-->
        <me:grainSize units="cm-1">20</me:grainSize>
        <!--...or by the total number of grains
        <me:numberOfGrains> 500 </me:numberOfGrains>-->
        <!--Specify increased energy range 
        <me:maxTemperature>6000</me:maxTemperature>-->
        <me:energyAboveTheTopHill>20.</me:energyAboveTheTopHill>
    </me:modelParameters>
    <me:control>
        <me:testDOS />
        <me:testMicroRates />
        <!--<me:printGrainDOS />-->
        <!--<me:printCellDOS />-->
        <me:printCollisionOperatorColumnSums />
        <!--<me:printCellkfE />-->
        <!--<me:printCellkbE />-->
        <!--<me:printGrainkfE />-->
        <!--<me:printGrainBoltzmann />-->
        <!--<me:printGrainkbE />-->
        <me:eigenvalues>0</me:eigenvalues>
    </me:control>
</me:mesmer>
