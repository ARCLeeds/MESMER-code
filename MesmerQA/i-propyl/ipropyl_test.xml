      </product>

      <me:MCRCMethod>MesmerILT</me:MCRCMethod>
      <me:preExponential>6.00e-12</me:preExponential>
      <me:activationEnergy  units="cm-1" reverse="true">546.D0</me:activationEnergy>
      <me:TInfinity>1.0</me:TInfinity>
      <me:nInfinity>0.0</me:nInfinity>

    </reaction>

  </reactionList>

