  <reactionList>

    <reaction id="R1">
      <reactant>
        <molecule ref="n-pentyl"  me:type="modelled" />
      </reactant>

      <product>
        <molecule ref="2-pentyl"  me:type="modelled" />
      </product>

      <me:MCRCMethod>SimpleRRKM</me:MCRCMethod>

      <me:transitionState>
        <molecule ref="n-pentyl=TS=2-pentyl" me:type="transitionState" />
      </me:transitionState>

    </reaction>

  </reactionList>

