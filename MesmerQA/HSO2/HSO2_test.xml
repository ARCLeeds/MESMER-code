    <me:energyAboveTheTopHill>20.</me:energyAboveTheTopHill>
  </me:modelParameters>
  <me:control>
    <me:testDOS />
    <me:testMicroRates />
    <me:printGrainDOS />
    <!--<me:printCellDOS />-->
    <!--<me:printReactionOperatorColumnSums />-->
    <me:printGrainkfE />
    <me:printGrainkbE />
    <me:printSpeciesProfile />
    <me:eigenvalues>0</me:eigenvalues>
  </me:control>


