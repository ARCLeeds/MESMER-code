<?xml version="1.0" encoding="utf-8"?>
<?xml-stylesheet type='text/xsl' href='mesmer1.xsl'?>
<me:mesmer xmlns="http://www.xml-cml.org/schema" xmlns:me="http://www.chem.leeds.ac.uk/mesmer">
  <!--Note that the thermodata is not real-->
  
  <moleculeList>
    <molecule id="H">
      <atom id="a1" elementType="H"/>
      <property dictRef="me:ZPE">
        <scalar units="kJmol-1">39.44</scalar>
      </property>
      <property dictRef="me:MW">
        <scalar>1</scalar>
      </property>
    </molecule>
    <molecule id="SO2">
      <atomArray>
        <atom id="a1" elementType="O"/>
        <atom id="a2" elementType="S"/>
        <atom id="a3" elementType="O"/>
      </atomArray>
      <bondArray>
        <bond atomRefs2="a1 a2" order="2"/>
        <bond atomRefs2="a2 a3" order="2"/>
      </bondArray>
      <property dictRef="me:MW">
        <scalar>64</scalar>
      </property>
      <property dictRef="me:rotConsts">
        <array units="cm-1">2.016 0.345 0.295</array>
      </property>
      <property dictRef="me:symmetryNumber">
        <scalar>2</scalar>
      </property>
      <property dictRef="me:vibFreqs">
        <array units="cm-1">
          518 1151 1362
        </array>
      </property>
      <property dictRef="me:ZPE">
        <scalar units="kJmol-1">39.44</scalar>
      </property>
    </molecule>
    <molecule id="HSO2">
      <atomArray>
        <atom id="a1" elementType="S"/>
        <atom id="a2" elementType="H"/>
        <atom id="a3" elementType="O"/>
        <atom id="a4" elementType="O"/>
      </atomArray>
      <bondArray>
        <bond atomRefs2="a1 a2" order="1"/>
        <bond atomRefs2="a1 a3" order="2"/>
        <bond atomRefs2="a1 a4" order="2"/>
      </bondArray>
      <property dictRef="me:MW">
        <scalar>65</scalar>
      </property>
      <property dictRef="me:ZPE">
        <scalar units="kJmol-1">24.38</scalar>
      </property>
      <property dictRef="me:rotConsts">
        <array units="cm-1">1.703 0.3 0.263</array>
      </property>
      <property dictRef="me:vibFreqs">
        <array units="cm-1">
         411 777 1027 1094 1578 2284
        </array>
      </property>
      <property dictRef="me:deltaEDown">
        <scalar units="cm-1">230.0</scalar>
      </property>
    </molecule>
    <molecule id="TS1">
      <atomArray>
        <atom id="a1" elementType="S"/>
        <atom id="a2" elementType="H"/>
        <atom id="a3" elementType="O"/>
        <atom id="a4" elementType="O"/>
      </atomArray>
      <bondArray>
        <bond atomRefs2="a1 a2" order="1"/>
        <bond atomRefs2="a1 a3" order="2"/>
        <bond atomRefs2="a1 a4" order="2"/>
      </bondArray>
      <property dictRef="me:MW">
        <scalar>65</scalar>
      </property>
      <property dictRef="me:ZPE">
        <scalar units="kJmol-1">178.6</scalar>
      </property>
      <property dictRef="me:rotConsts">
        <array units="cm-1">1.22 0.321 0.275</array>
      </property>
      <property dictRef="me:vibFreqs">
        <array units="cm-1">
          347 358 517 117 1381
        </array>
      </property>
    </molecule>
    <molecule id="Ar">
      <atom elementType="Ar"/>
      <propertyList>
        <property dictRef="me:epsilon">
          <scalar>114</scalar>
        </property>
        <property dictRef="me:sigma">
          <scalar>3.47</scalar>
        </property>
        <property dictRef="me:MW">
          <scalar>39.45</scalar>
        </property>
      </propertyList>
    </molecule>
  </moleculeList>
  <reactionList>
    <reaction id="R1" reversible="true">
      <reactant>
        <molecule ref="H" me:type="reactant" />
      </reactant>
      <reactant>
        <molecule ref="SO2" me:type="excessReactant" />
      </reactant>
      <product>
        <molecule ref="HSO2" me:type="modelled" />
      </product>
      <me:transitionState>
        <molecule ref="TS1" me:type="transitionState" />
      </me:transitionState>
      <me:MCRCMethod>Simple RRKM</me:MCRCMethod>
      <me:threshold>41.56</me:threshold>
    </reaction>
  </reactionList>
  <me:conditions>
    <me:bathGas>Ar</me:bathGas>
    <me:temperature>300</me:temperature>
    <me:conc>1e8</me:conc>
  </me:conditions>
  <me:modelParameters>
    <me:grainSize units="cm-1"> 100 </me:grainSize>
    <!--<me:numberOfGrains> 500 </me:numberOfGrains>-->
    <me:maxTemperature>3000</me:maxTemperature>
  </me:modelParameters>
    <me:control>
