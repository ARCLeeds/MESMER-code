  <reactionList>
    <reaction id="R1" reversible="true">
      <reactant>
        <molecule ref="H" me:type="deficientReactant" />
      </reactant>
      <reactant>
        <molecule ref="SO2" me:type="excessReactant" />
      </reactant>
      <product>
        <molecule ref="HSO2" me:type="modelled" />
      </product>
      <me:transitionState>
        <molecule ref="TS1" me:type="transitionState" />
      </me:transitionState>
      <me:excessReactantConc>1e14</me:excessReactantConc>
      <me:MCRCMethod>Simple RRKM</me:MCRCMethod>
    </reaction>
  </reactionList>
  <me:conditions>
