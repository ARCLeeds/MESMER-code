  </moleculeList>

  <reactionList>
    <reaction id="R1" reversible="true">
      <reactant>
        <molecule ref="H" role="deficientReactant" />
      </reactant>
      <reactant>
        <molecule ref="SO2" role="excessReactant" />
      </reactant>
      <product>
        <molecule ref="HSO2" role="modelled" />
      </product>
      <me:transitionState>
        <molecule ref="TS1" role="transitionState" />
      </me:transitionState>
      <me:excessReactantConc>1e14</me:excessReactantConc>
      <me:MCRCMethod name="SimpleRRKM"/>
    </reaction>
    <reaction id="R2" reversible="true">
      <reactant>
        <molecule ref="H" role="deficientReactant"  />
      </reactant>
      <reactant>
        <molecule ref="SO2" role="excessReactant"  />
      </reactant>
      <product>
        <molecule ref="HOSO" role="modelled"  />
      </product>
      <me:transitionState>
        <molecule ref="TS2" role="transitionState" />
      </me:transitionState>
      <me:MCRCMethod name="SimpleRRKM"/>
    </reaction>
    <reaction id="R4" reversible="true">
      <reactant>
        <molecule ref="HSO2" role="modelled"   />
      </reactant>
      <product>
        <molecule ref="HOSO" role="modelled"   />
      </product>
      <me:transitionState>
        <molecule ref="TS4" role="transitionState" />
      </me:transitionState>
      <me:MCRCMethod name="SimpleRRKM"/>
    </reaction>
    <reaction id="R6">
      <reactant>
        <molecule ref="HOSO" role="modelled"/>
      </reactant>
      <product>
        <molecule ref="OH" role="sink"/>
      </product>
      <product>
        <molecule ref="SO" role="sink" />
      </product>
      <me:MCRCMethod xsi:type="me:MesmerILT">
        <me:preExponential units="cm3molecule-1s-1">52.40e-11</me:preExponential>
        <me:activationEnergy  units="cm-1" reverse="true">0.0</me:activationEnergy>
        <me:TInfinity>300.0</me:TInfinity>
        <me:nInfinity>0.4</me:nInfinity>
      </me:MCRCMethod>
    </reaction>
  </reactionList>

  <me:conditions>
    <me:bathGas>Ar</me:bathGas>
    <me:PTs>
      <me:PTpair units="Torr" P="100000000" T="200" precision ="dd" />
    </me:PTs>
    <me:InitialPopulation>
      <me:molecule ref="H" population="1.0" />
    </me:InitialPopulation>
  </me:conditions>

  <me:modelParameters>
    <me:grainSize units="cm-1"> 100 </me:grainSize>
    <me:energyAboveTheTopHill>20.</me:energyAboveTheTopHill>
  </me:modelParameters>

  <me:control>
    <me:testDOS />
    <me:testMicroRates />
    <me:eigenvalues>5</me:eigenvalues>
    <me:ForceMacroDetailedBalance />
  </me:control>

  <me:control>
    <me:calcMethod units="kJ/mol" xsi:type="me:ThermodynamicTable">
      <me:Tmin>75</me:Tmin>
      <me:Tmid>600</me:Tmid>
      <me:Tmax>1050</me:Tmax>
      <me:Tstep>75</me:Tstep>
    </me:calcMethod>
  </me:control>

</me:mesmer>
