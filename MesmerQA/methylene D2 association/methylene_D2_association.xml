        <molecule id="D2">
            <atomarray>
                <atom id="a1" elementType="D" />
                <atom id="a2" elementType="D" />
            </atomarray>
            <bondarray>
                <bond atomRefs2="a1 a2" order="1" />
            </bondarray>
            <propertyList>
                <property dictRef="me:ZPE">
                    <scalar units="kJ/mol">-1.173030</scalar>
