                <bond atomRefs2="a1 a5" order="1" />
            </bondarray>
            <propertyList>
                <property dictRef="me:ZPE">
                    <scalar units="kJ/mol">-40.498852</scalar>
                </property>
                <property dictRef="me:rotConsts">
                    <array units="cm-1">4.3029 3.5059 3.0501</array>
                </property>
